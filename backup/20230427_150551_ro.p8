function _init()
  cls()

  palt(11, true) -- beige color as transparency is true
  palt(0, false) -- black color as transparency is false

	init_joka()
	init_zivuljke()
end

function _update()
	update_joka()
	update_camera()
end

function _draw()
	cls()
	map()
	draw_joka()
	draw_zivuljke()
	--debug
	--is_solid("full",joka,0,0,{},true)

	
end
