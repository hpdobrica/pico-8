function init_dog(name,s,x,y)
 return {
 	name=name,
		s=s,
		w=15,
		h=15,
		ox=0,
		oy=0,
		x=x,
		y=y,
		sx=false,
		sy=false
	}
end



function init_zivuljke()
	zivuljke={
		init_dog("julija",64,100,60),
		init_dog("dora",68,145,105),
		init_dog("belka",66,250,155),
		init_dog("prca",72,162,15),
		init_dog("lisa",70,70,70),
		init_dog("klara",74,100,170),
		init_dog("sofija",76,115,180),
		init_dog("bila mica",78,130,170),
		init_dog("mezimica",106,150,170),
		init_dog("sarka",108,60,120),
		init_dog("smokvica",110,235,7),

	}
end

function draw_zivuljke()
	for i = 1,#zivuljke do
		local z=zivuljke[i]
		spr(z.s, z.x, z.y,flr((z.w+z.ox)/8)+1,flr((z.h+z.oy)/8)+1,z.sx,z.sy)
	end

end
