function _init()
  cls()

  palt(7, true) -- beige color as transparency is true
  palt(0, false) -- black color as transparency is false

	init_joka()
end

function _update()
	update_joka()
end

function _draw()
	map()
	draw_joka()
	
end
-->8
--joka


function init_joka()
	joka={
		spr={
			id=1,
			w=2,
			h=3,
		},
		x=15,
		y=15
	}
end

function update_joka()
	if btn(1) then
		joka.x+=1
	end
end

function draw_joka()
	s=joka.spr
	--spr(s.id,joka.x,joka.y,s.w,s.h)
	sspr(0,2,)
end
