--joka


function init_joka()
	joka={
		s=1,
		w=15,
		h=23,
		ox=0,
		oy=0,
		col= {
			w=10,--15
			h=10,--15
			ox=3,--0
			oy=13,--8
		},
		x=160,
		y=60,
		sx=false,
		sy=false,
		speed=3,
		touching=nil
	}
end

function update_joka()
	local j=joka
	local dx,dy=0,0
 if btn(⬅️) then
  j.sx = true  -- flip sprite on x
  dx=-j.speed
 end
 if btn(➡️) then
  j.sx = false
  dx+=j.speed  -- dx=dx+1
 end
 if btn(⬆️) then
  j.sy = false
  dy=-j.speed
 end
 if btn(⬇️) then
  //j.sy = true  -- flip sprite on y
  dy+=j.speed  -- dy=dy+1
 end
 if btn(❎) then
 	if joka.touching != nil then
 		joka.touching.text="test"
 	end
 	
 end
 
 -- there's no map-tile with
 -- flag "7" on the players'
 -- position + dx ? then move:
 if not is_solid("full",j,dx,0,7) then
  j.x+=dx
 end
 -- there's no map-tile with
 -- flag "7" on the players'
 -- position + dy ? then move:
 if not is_solid("full",j,0,dy,7) then
  j.y+=dy
 end
	
end

function draw_joka()
	local j=joka
	--spr(s.id,joka.x,joka.y,s.w,s.h)
	--sspr(8,0,16,24,joka.x,joka.y,16,24)
	spr(j.s, j.x, j.y,flr((j.w+j.ox)/8)+1,flr((j.h+j.oy)/8)+1,j.sx,j.sy)
end
