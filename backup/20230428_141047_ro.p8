--util

function hcenter(s,x,y)
  -- screen center minus the
  -- string length times the 
  -- pixels in a char's width,
  -- cut in half
  return x-#s*2
end

function vcenter(s,x,y)
  -- screen center minus the
  -- string height in pixels,
  -- cut in half
  return 61
end
